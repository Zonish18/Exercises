float x, y;
float xStep = 5;
float yStep = 200;
float a, a_;
int num = 320;
void setup() {
size (900, 900);
}
void draw() {
background (15, 20, 30);
strokeWeight(5);
  int n = 0;
  while(n<num) {
    stroke(0-0-255, 115-127*sin(radians(a)));
    line(x, y, x, y+yStep);
    x+=xStep;
    if (x>width){
      x=xStep/2;
      y+=yStep;
    }  
    if (y>=height){
      y=0;
      a=0;
    }
      n++;
    a+=a_;
  }
  a_+=0.1;
}
